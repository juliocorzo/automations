generator client {
  provider = "prisma-client-js"
}
 
datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model PerformanceMetric {
  id                              Int         @id @default(autoincrement())
  inletTemperature                Float
  outletTemperature               Float
  coolingPower                    Int
  flowRate                        Float
  cpuTemperature                  Float
  cpuLoad                         Float
  cpuPower                        Int
  gpuTemperature                  Float
  gpuLoad                         Float
  gpuPower                        Int
  memoryLoad                      Float
  airMotherboardFrontTemperature  Float
  airMotherboardBackTemperature   Float
  airCaseTemperature              Float
  createdAt                       DateTime
}